<h2>Edit Report </h2>

<form method="post" action="/reports/<%=@report.id%>">
  <h3>Report:</h3><input id="hidden" type="hidden" name="_method" value="PATCH">
  <input type="text" name="content" value="<%= @report.content %>"></br>
  <h3>Location:</h3><input type="text" name="location" value="<%= @report.location %>">
  </br>
  </br>
  <input type="submit" value="Submit">
</form>
